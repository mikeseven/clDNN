"sync": {
    "copy" : [
        {
            "qual":       "*",
            "src_prefix": ".",
            "dst_prefix": ".",
            "include":    [],
            "include_re": [],
            "exclude":    ["common/intel_ocl_icd/6.2", "common/FreeImage", "**/android", "**/FindBoost.cmake",
                           "inference_engine", "src/caps/internal", "src/caps/private", "examples", "utils", "*_ie.sh", "*_ie.bat", "*_ie.ps1"],
            "exclude_re": [],
            "ignore":     ["**/.git", "**/.gitmodules", "build", "**/*.pyc", "**/*~", "UnixMk", "**/.idea"],
            "ignore_re":  []
        },
        {
            "qual":       "utils",
            "src_prefix": "utils",
            "dst_prefix": "utils",
            "include":    ["utils/build/cmake", "utils/codegen"],
            "include_re": [],
            "exclude":    [],
            "exclude_re": [],
            "ignore":     ["**/.git", "**/.gitmodules", "build", "**/*.pyc", "**/*~", "UnixMk", "**/.idea"],
            "ignore_re":  []
        }
    ],
    "update_version": [
        {
            "file":     ["version.json"],
            "position": "Build",
            "step":     1
        }
    ],
    "process_sections": [
        {
            "file":             ["CMakeLists.txt"],
            "exclude_sections": ["GenericSample", "Android", "Examples", "FreeImage"],
            "type":             "CMake",
            "key":              "PPC",
            "tab_size":         2,
            "use_tabs":         false,
            "eol":              "Unix"
        }
    ],
    "disabled__format_file": [
        {
            "file":             ["src/**/.cpp", "src/**/*.hpp", "src/**/*.h"],
            "type":             "Cxx",
            "trim":             true,
            "normalize_indent": "Spaces",
            "tab_size":         4,
            "eol":              "Unix"
        }
    ],
    "disabled__chmod_staged": [
        {
            "file": ["**/.sh", "**/*.py"],
            "mode": "+x",
            "vcs":  "git"
        }
    ]
}