# Please note that this file is parsed by simple parser written directly in CMake. Do not use advanced constructs.
{
    major: 1,          # clDNN major version (major version of API).
    minor: 3,          # clDNN minor version (correlated with major API version of Inference Engine).
    build: 3,          # clDNN build version (correlated with ordinal numeber of public release of clDNN).
    revision_base: 10, # Offset that will be subtracted from environment variable provided by build system.
    revision_min: 0    # Minumum value of revision. Computed value of revision will be clamped from below by this value.
}